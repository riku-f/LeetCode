class Solution(object):
    def searchInsert(self, nums, target):
        l = len(nums)
        if target>nums[-1]: return l
        count=0
        while count<l:
            if nums[count]>=target: return count
            else: count+=1
                
        """
        :type nums: List[int]
        :type target: int
        :rtype: int
        """
        
